using System;

class AbundantNo4
{
    static void Main()
    {
        int number;//talkh the inpot
        int sum=0;

        Console.WriteLine("Enter number:");
        number=Convert.ToInt32(Console.ReadLine());

        //find thr dvsors
        for(int i=1;i<number;i++)
        {
            if(number%i==0)
                sum=sum+i;
        }

        //chechr the abundnrt 
        if(sum>number)
            Console.WriteLine("Abundant Number");
        else
            Console.WriteLine("Not an Abundant Number");
    }
}
